server {
    root /home/tumani1/workspace/videobase;
    index index.html index.htm;

    access_log /var/log/nginx/videobase.access.log;
    error_log /var/log/nginx/videobase.error.log;

    server_name videobase.local;

    location /production/static/admin/ {
        alias /home/tumani1/workspace/videobase/static/admin;
        autoindex on;
        expires 30d;
    }

    location /static/upload/ {
        alias /home/tumani1/workspace/videobase/static/upload/;
        autoindex on;
        expires 30d;
    }

    location / {
        uwsgi_pass unix:///tmp/videobase_uwsgi.sock;
        include uwsgi_params;
    }
}