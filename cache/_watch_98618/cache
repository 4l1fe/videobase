        da.track.configure({"go":"\/go${url}\/~\/${uid}:${area}","q":"\/q\/~\/${track}:${uid}:${prev}","exclude":"~|^#","log":{"go":"cache\/sphinx\/go.log","q":"cache\/sphinx\/q.log"}});
