        <script>
            $(document).ready(function() {
                getHeaderHeight = function() {
                    return $('.header').height();
                }

                Runner.sendAdcErotica = function(el) {
                    if (el.target.href.indexOf('plus') >= 0) {
                        xt_adc(this, 'PUB-[header_old]-[ivi_plus_erotics]');
                    } else if (el.target.href.indexOf('series') >= 0) {
                        xt_adc(this, 'PUB-[header_old]-[series_erotika]');
                    } else {
                        xt_adc(this, 'PUB-[header_old]-[movies_erotika]');
                    }
                }

                //Инициация автокомплита:
                var $input = $('#search-input');
                $input.focus(function(){
                    if (this.value == this.getAttribute('title'))
                    {
                        this.value = '';
                    }
                });
                var search = new da.autocomplete({
                    input: $input,
                    success: function(data, ul)
                    {
                        //highlight words:
                        var re = new RegExp('(' + $input.val() + ')', 'ig');
                        ul.find('strong').each(function(){
                            this.innerHTML = this.innerHTML.replace(re, '<span class="highlight">$1</span>');
                        });
                        return ul;
                    },
                    //Draw:
                    section: da.feed.jqtpl('#autocomplete_section'),
                    content: da.feed.jqtpl('#autocomplete_content'),
                    person: da.feed.jqtpl('#autocomplete_person'),
                    //CSS:
                    className: 'ac_results',
                    width: 250,
                    marginTop: function() {
                        return Math.floor(getPlatesHeight() + getHeaderHeight() / 2)
                    }(),
                    marginLeft: -12
                });
            });
        </script>
