    var debug = debug || false;

    /**
     * Advertising environment. 
     *
     * @class AdvContext
     */
    var AdvContext = function() {};
    AdvContext.prototype.sid = 184318;
    AdvContext.prototype.sz = "main";
    AdvContext.prototype.pz = 1;
    AdvContext.prototype.bn = 1;
    AdvContext.prototype.bt =52;
    AdvContext.prototype.custom = {};
    AdvContext.prototype.asDict = function() {
    	var key, result = {};
    	for(key in this) {
    		if(this.hasOwnProperty(key)) {
    			continue;
    		}
    		if(typeof this[key] === 'function') {
    			continue;
    		}
    		result[key] = this[key];
    	}
    	return result;
    };


    var counter = 0,
        advFunction = null;

    /**
     * @method adv240
     *
     * @param {String|DOMElement} selector Elements will contain banner.
     * @param {Number} catalogue
     * @param {Number} channel
     * @param {Number} majorGroup
     * @param {Number} major
     * @param {Number} keyword
     * @param {Number} duration Show time.
     * @param {*} bn Unknown purpose.
     *
     * @return null
     */
    var adv240 = function(selector, catalogue, channel, majorGroup, major, keyword, duration, bn) {

    	duration = duration * 1000;

    	debug && console.group('Function `adv240` occured');
	    	debug && console.groupCollapsed('Arguments');
