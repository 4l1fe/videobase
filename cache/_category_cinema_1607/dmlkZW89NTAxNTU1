	    		debug && console.log("Major:", major);
	    		debug && console.log("Keyword:", keyword);
	    		debug && console.log("Duration:", duration);
	    		debug && console.log("Bn:", bn);
	    	debug && console.groupEnd('Arguments');

	    var advParams = this.asDict();
		advParams.custom[11] = majorGroup;
		advParams.custom[12] = catalogue;
		advParams.custom[13] = major;
		advParams.custom[14] = channel;
		advParams.keyword = keyword;
		advParams.bn = bn;

        elementId = 'banner_' + (counter++);

		// Cleanin' up banner area
        var bannerElement = $('<div id="' + elementId + '" class="b thebanner240"></div>');
		$(selector).replaceWith(bannerElement);

		debug && console.log('Out coming params', advParams);

		var result = new adriver(elementId, advParams);

			debug && console.groupCollapsed('Returns');
				debug && console.log('Result:', result);
			debug && console.groupEnd('Returns');

//        if(result) {
