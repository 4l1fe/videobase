    var AdvContext = function() {};
    AdvContext.prototype.sid = 184318;
    AdvContext.prototype.sz = "main";
    AdvContext.prototype.pz = 1;
    AdvContext.prototype.bn = 1;
    AdvContext.prototype.bt =52;
