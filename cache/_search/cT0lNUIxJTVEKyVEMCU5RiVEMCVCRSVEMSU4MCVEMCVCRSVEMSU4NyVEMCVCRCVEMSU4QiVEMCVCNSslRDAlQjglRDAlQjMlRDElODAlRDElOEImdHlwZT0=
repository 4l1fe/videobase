				_gaq.push(function () {
					function setCookie(c_name, value, exdays, exdate) {
						if (!exdate) {
							exdate = new Date();
						}
						exdate.setDate(exdate.getDate() + exdays);
						var c_value = escape(value) + ((exdays === null) ? "" : "; expires=" + exdate.toUTCString());
						document.cookie = c_name + "=" + (c_value + "; path=/; domain=zoomby.ru");
					}

					function getCookie(c_name) {
						var i, x, y, ARRcookies = document.cookie.split(";");
						for (i = 0; i < ARRcookies.length; i++) {
							x = ARRcookies[i].substr(0, ARRcookies[i].indexOf("="));
							y = ARRcookies[i].substr(ARRcookies[i].indexOf("=") + 1);
							x = x.replace(/^\s+|\s+$/g, "");
							if (x === c_name) {
								return unescape(y);
							}
						}
						return null;
					}

					function getSource() {
						var source = /utm_source=([^&]+)|gclid=([^&]+)/.exec(document.location.search);
						if (source && (source.length === 2)) {
							return source[1];
						} else {
							source = /utmgclid=([^\|]+)|utmcsr=([^\|]+)/.exec(getCookie('__utmz'));
							if (source && (source.length === 3)) {
								return (source[1] || source[2]);
							}
						}
					}

					function setZoombyCookie(r, days, name) {
						var result = null;
						if (r) {
							var s_cookie = r.split('_');
							var s = s_cookie[0],
								t = s_cookie[1],
								end_month = s_cookie[2],
								end_day = s_cookie[3];
							t = parseInt(t, 10) + 1;
							result = s + "_" + t;
							var d = new Date();
							end_month = parseInt(end_month, 10);
							end_day = parseInt(end_day, 10);
							d.setMonth(end_month);
							d.setDate(end_day);
							setCookie(name, [s, t, end_month, end_day].join('_'), days, d);
						} else {
							var s = getSource();
							if (s) {
								var t = 1;
								result = s + "_" + t;
								var d = new Date();
								d.setDate(d.getDate() + days);
								setCookie(name, [s, t, d.getMonth(), d.getDate()].join('_'), days);
							}
						}
						return result;
					}
					try {
						var r_mon = getCookie('returns_month');
						var r_per = getCookie('returns_persistent');
						var r_sess = getCookie('returns_session');
						if (!r_sess) {
							var monthly_cookie = setZoombyCookie(r_mon, 30, 'returns_month');
							var per_cookie = setZoombyCookie(r_per, 180, 'returns_persistent');
							if (monthly_cookie) {
								_gaq.push(['_setCustomVar', 4, 'r_m', monthly_cookie, 2]);
							}
							if (per_cookie) {
								_gaq.push(['_setCustomVar', 5, 'r_p', per_cookie, 2]);
							}
							setCookie('returns_session', 'tick');
						}
					} catch (e) {}
				});
